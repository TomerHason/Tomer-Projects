   "source": [
    "merger = PyPDF2.PdfFileMerger()\n",
    "for file in os.listdir(os.curdir):\n",
    "    if file.endswith(\".pdf\"):\n",
    "        merger.append(file)\n",
    "merger.write(\"YouWelcome.pdf\")"
   ]
